{
  "name": "@stargazer/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Core review engine library for Stargazer - AI-powered code review",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./gemini/client": {
      "import": "./dist/gemini/client.js",
      "types": "./dist/gemini/client.d.ts"
    },
    "./gemini/types": {
      "import": "./dist/gemini/types.js",
      "types": "./dist/gemini/types.d.ts"
    },
    "./review/reviewer": {
      "import": "./dist/review/reviewer.js",
      "types": "./dist/review/reviewer.d.ts"
    },
    "./review/schemas": {
      "import": "./dist/review/schemas.js",
      "types": "./dist/review/schemas.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "echo 'Placeholder - not yet implemented'",
    "check": "echo 'Placeholder - not yet implemented'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "code-review",
    "ai",
    "gemini",
    "stargazer"
  ],
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.34.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3",
    "tsx": "^4.21.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5"
  }
}
